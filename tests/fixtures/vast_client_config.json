{
  "provider": "test_provider",
  "publisher": "test_publisher",
  "enable_tracking": true,
  "enable_parsing": true,
  "parser": {
    "recover_on_error": true,
    "encoding": "utf-8",
    "strict_xml": false
  },
  "tracker": {
    "timeout": 5.0,
    "macro_formats": ["[{macro}]", "${{{macro}}}"],
    "static_macros": {
      "PUBLISHER_ID": "pub-test-001",
      "PLATFORM": "ctv"
    },
    "macro_mapping": {}
  },
  "session": {
    "mode": "headless",
    "enable_auto_quartiles": true,
    "headless_tick_interval_sec": 0.1,
    "max_session_duration_sec": 0
  }
}
